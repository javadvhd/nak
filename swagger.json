  "info": {
    "title": "NestJS MongoDB API",
    "description": "API documentation",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "access-token": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "example": "John",
            "description": "First name"
          },
          "lastName": {
            "type": "string",
            "example": "Doe",
            "description": "Last name"
          },
          "userName": {
            "type": "string",
            "example": "jdoe",
            "minLength": 4
          },
          "password": {
            "type": "string",
            "example": "secret123",
            "minLength": 6
          }
        },
        "required": [
          "firstName",
          "lastName",
          "userName",
          "password"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "example": "jdoe",
            "description": "Username of the user"
          },
          "password": {
            "type": "string",
            "example": "secret123",
            "description": "User password"
          }
        },
        "required": [
          "userName",
          "password"
        ]
      },
      "CreateAttributeDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "color"
          },
          "values": {
            "example": [
              "red",
              "blue"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "name",
          "values"
        ]
      },
      "CreateSkuDto": {
        "type": "object",
        "properties": {
          "model": {
            "type": "string",
            "example": "A1"
          },
          "price": {
            "type": "string",
            "example": "100"
          },
          "numberInStock": {
            "type": "string",
            "example": "50"
          }
        },
        "required": [
          "model",
          "price",
          "numberInStock"
        ]
      },
      "UpdateSkuDto": {
        "type": "object",
        "properties": {}
      },
      "AttrDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "color",
            "description": "Name of the attribute"
          },
          "values": {
            "example": [
              "red",
              "blue"
            ],
            "description": "Allowed values for this attribute",
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "name",
          "values"
        ]
      },
      "CreateProductDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "T-Shirt",
            "description": "Name of the product"
          },
          "skusIds": {
            "example": [
              "skuId1",
              "skuId2"
            ],
            "description": "List of SKU IDs for this product",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "attributes": {
            "description": "Array of attribute objects with name & values",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AttrDto"
            }
          }
        },
        "required": [
          "name",
          "skusIds",
          "attributes"
        ]
      },
      "UpdateProductDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
}